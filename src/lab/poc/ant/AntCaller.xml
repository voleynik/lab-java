<?xml version="1.0" encoding="UTF-8"?>
<project name="AntCaller" default="AntCaller" basedir=".">

	<target name="AntCaller">
		<!-- OS environment variables -->
		<property environment="env"/>
		<echo message="JAVA_HOME = ${env.JAVA_HOME}"/>
		<echo message="ANT_HOME = ${env.ANT_HOME}"/>
		<!-- Time stamps -->
		<tstamp><format property="yyMMdd" pattern="yyMMdd"/></tstamp>
		<echo message="yyMMdd = ${yyMMdd}"/>
		<tstamp><format property="HHmmss" pattern="HHmmss"/></tstamp>
		<echo message="HHmmss = ${HHmmss}"/>
	</target>
	
</project>